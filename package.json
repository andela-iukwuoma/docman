{
  "name": "docman",
  "version": "1.0.0",
  "description": "Full-stack document management system",
  "main": "index.js",
  "scripts": {
    "start": "babel-node ./server/index.js",
    "start:dev": "nodemon --watch server --exec babel-node -- ./bin/www",
    "test": "npm run setup:test && NODE_ENV=test nyc mocha server/tests/**/*.js client/tests/testSetup.js client/tests/**/* --timeout 20000",
    "test:server": "npm run setup:test && NODE_ENV=test nyc mocha server/tests/**/*.spec.js --timeout 20000",
    "test:client": "NODE_ENV=test nyc mocha client/tests/testSetup.js client/tests/**/*",
    "setup:test": "npm run db:migrate:test && NODE_ENV=test sequelize db:seed:all",
    "build": "babel-node tools/build.js",
    "postinstall": "npm run build && npm run db:migrate",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "server/tests",
      "client/tests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-iukwuoma/docman.git"
  },
  "keywords": [
    "document",
    "management"
  ],
  "author": "Ignatius Ukwuoma <ignatius.ukwuoma@andela.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-iukwuoma/docman/issues"
  },
  "homepage": "https://github.com/andela-iukwuoma/docman#readme",
  "dependencies": {
    "animate.css": "^3.5.2",
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "6.8.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chai": "^4.0.0",
    "chai-http": "^3.0.0",
    "cheerio": "^1.0.0-rc.1",
    "clean-webpack-plugin": "^0.1.16",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "expect": "^1.20.2",
    "expect-enzyme": "^1.0.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "faker": "^4.1.0",
