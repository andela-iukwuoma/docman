{
  "name": "docman",
  "version": "1.0.0",
  "description": "Full-stack document management system",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch server --exec babel-node -- ./bin/www",
    "test": "npm run setup:test && NODE_ENV=test nyc mocha server/tests/**/*.js client/test/testSetup.js client/test/**/* --timeout 20000",
    "test:server": "npm run setup:test && NODE_ENV=test nyc mocha server/tests/**/*.js --timeout 20000",
    "setup:test": "npm run db:migrate:test && NODE_ENV=test sequelize db:seed:all",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "server/tests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-iukwuoma/docman.git"
  },
  "keywords": [
    "document",
    "management"
  ],
  "author": "Ignatius Ukwuoma <ignatius.ukwuoma@andela.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-iukwuoma/docman/issues"
  },
  "homepage": "https://github.com/andela-iukwuoma/docman#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "6.8.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^7.4.1",
    "material-ui": "^0.16.7",
    "morgan": "^1.8.2",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^1.0.0",
    "react-transition-group": "^1.1.3",
    "redux": "^3.5.2",
    "redux-thunk": "^2.0.1",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0",
    "chai-http": "^3.0.0",
    "cheerio": "^1.0.0-rc.1",
    "clean-webpack-plugin": "^0.1.16",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "faker": "^4.1.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "nyc": "^10.3.2",
    "react-hot-loader": "^1.3.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "sass-loader": "^6.0.5",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
